import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStream;
import java.io.InputStreamReader;
import java.util.Scanner;

public class HelloWorld{

	public static void main (String[] args)
	{
	int a;
	Scanner sn = new Scanner(System.in);
		System.out.println("Enter the a numbers:");  //getting input values 
		a=sn.nextInt();
		

        if (a > 0)
        {
        	System.out.println("You Have Entered Positive Value");
        }
        else if (a < 0)
        {
            System.out.println("You Have Entered Negative Value");
        }
        else 
        {
            System.out.println("You Have Entered zero Value");
        }
        }
        }
		
